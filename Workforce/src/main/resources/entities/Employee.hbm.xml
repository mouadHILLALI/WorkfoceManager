<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-5.0.dtd">
<hibernate-mapping>
    <class name="workforcemanger.workforce.model.Employee" table="employees">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name" column="name"/>
        <property name="email" column="email"/>
        <property name="phone" column="phone"/>
        <property name="poste" column="poste"/>
        <property name="departementID" column="departementID"/>
    </class>
</hibernate-mapping>
